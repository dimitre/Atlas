size(1024, 1024)
background(0, 0)
# background(1)
m = 0.05
rect(WIDTH * m, HEIGHT * m, WIDTH * (1-m*2), HEIGHT * (1-m*2), roundness=0.15)

fill(1,0,.3)
r = 300
ellipse(WIDTH*.33 -r*.5, HEIGHT*.33 -r*.5, r, r)
r = 150
ellipse(WIDTH*.67 -r*.5, HEIGHT*.67 -r*.5, r, r)

stroke(1,0,.3)
strokewidth(10)
line(WIDTH*.33, HEIGHT*.33, WIDTH*.67, HEIGHT*.67 )

    
export("track.png")    